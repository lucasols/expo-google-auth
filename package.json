{
  "name": "@ls-stack/expo-google-auth",
  "version": "0.1.0",
  "description": "Hybrid Google authentication for Expo: native GoogleSignIn on iOS, Credential Manager on Android.",
  "type": "module",
  "main": "dist/src/index.mjs",
  "types": "dist/src/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.mts",
      "default": "./dist/src/index.mjs"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.mts",
      "default": "./dist/plugin/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"plugin/**/*.ts\"",
    "prepare": "pnpm run build",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "keywords": [
    "expo",
    "react-native",
    "google",
    "authentication",
    "credential-manager"
  ],
  "author": "Lucas Oliveira Santos",
  "license": "MIT",
  "peerDependencies": {
    "expo": "^54.0.0",
    "react": "^18.2.0",
    "react-native": "^0.76.0"
  },
  "dependencies": {
    "@expo/config-plugins": "^8.0.5",
    "expo": "~54.0.22",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.12.0",
    "rimraf": "^5.0.5",
    "tsdown": "^0.16.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@10.15.0"
}
